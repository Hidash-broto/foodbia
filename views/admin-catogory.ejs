<div class="container-fluid pt-4 px-4">
    <div class="row ">
        <div class="col-xl-2">

        </div>
<div class="col-sm-12 col-xl-6 ml-5">
  <% if(error){ %>
    <h5 class="text-danger"><%= error%></h5>
    <% } %>
<form method="post" action="/addCatogary" enctype="multipart/form-data" >
    <div class="form-group">
      <label for="exampleInputEmail1">Name of Catogary</label>
      <input required type="text" class="form-control" id="exampleInputEmail1"  placeholder="Name" name="name">
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Discription</label>
      <input required type="text" class="form-control form-control-lg" name="discription" >
    </div>
    <div class="form-group">
      <input type="file"  name="image">
    </div>
    <div class="form-group mt-3" >
    <select required id="inputState" class="form-control" name="status">
        <option selected>List</option>
        <option>Unlist</option>
      </select>
    </div>
    <button type="submit" class="btn btn-primary mt-2">Submit</button>
  </form>
</div>
</div>
</div>
