   <script>
    function myFunction(value) {
      console.log(value);
      fetch(`/searching?category=${value}`,{
        method:'get',
        headers:{
          'Content-type': 'application/json'
        },
        body:JSON.stringify()
      }).then(res=>{
        return res.json()
      }).then((data)=>{
        console.log(data);
        document.getElementById('searchData').innerHTML = ''
        if (value.length == 0) {
          document.getElementById('searchData').innerHTML = ''
        }else{
          if(data.length == 0){
            document.getElementById('searchData').innerHTML = 'Not Found'
          }else{
            data.forEach(element => {
              document.getElementById('searchData').innerHTML += `<li style = 'margin-left:170px; text-decoration: none;'><a href='/productList/${element._id}'> ${element.name} </a> </li>`
            });
          }
        }

      }).catch(err => console.log(err));
    }
   </script>
    
   
   
    
